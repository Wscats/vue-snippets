{
	"name": "vue",
	"displayName": "Vue 3 Snippets",
	"description": "This extension adds Vue 3 and Vue 2 Code Snippets into Visual Studio Code.",
	"icon": "public/logo.png",
	"author": {
		"name": "Eno Yao",
		"email": "kalone.cool@gmail.com",
		"url": "https://github.com/Wscats"
	},
	"publisher": "Wscats",
	"version": "0.0.3",
	"license": "MIT",
	"engines": {
		"vscode": "^1.40.0"
	},
	"keywords": [
		"vue",
		"vue2",
		"vue3",
		"vuejs",
		"vue.js",
		"composition-api",
		"composition",
		"function-api",
		"function",
		"html",
		"javascript",
		"snippet",
		"api",
		"cnode"
	],
	"homepage": "https://github.com/wscats/vue-snippets#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/wscats/vue-snippets"
	},
	"bugs": {
		"url": "https://github.com/wscats/vue-snippets/issues",
		"email": "kalone.cool@gmail.com"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Formatters"
	],
	"contributes": {
		"languages": [
			{
				"id": "vue",
				"aliases": [
					"Vue",
					"vue"
				],
				"extensions": [
					".vue"
				],
				"configuration": "./syntaxes/vue.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "vue",
				"scopeName": "text.html.vue",
				"path": "./syntaxes/vue.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "html",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "html",
				"path": "./snippets/html.json"
			},
			{
				"language": "html",
				"path": "./snippets/vue.json"
			},
			{
				"language": "vue",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "vue",
				"path": "./snippets/vue.json"
			},
			{
				"language": "vue",
				"path": "./snippets/html.json"
			}
		]
	},
	"scripts": {
		"build": "vsce package"
	}
}